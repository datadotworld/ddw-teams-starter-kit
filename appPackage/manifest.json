{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "manifestVersion": "1.16",
  "version": "1.0.0",
  "id": "${{TEAMS_APP_ID}}",
  "packageName": "com.yr-company.aice.starter.kit",
  "developer": {
    "name": "YR Company",
    "websiteUrl": "https://yr-company.com",
    "privacyUrl": "https://yr-company.com/legal/privacy/",
    "termsOfUseUrl": "https://yr-company.com/legal/terms/"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "AICEKit${{APP_NAME_SUFFIX}}",
    "full": "data.world AI Context Engine Starter Kit"
  },
  "description": {
    "short": "Starter kit for data.world's AI Context Engine",
    "full": "A starter kit for building your own app to interact with data.world's AI Context Engine."
  },
  "accentColor": "#6290c3",
  "bots": [
    {
      "botId": "${{BOT_ID}}",
      "scopes": ["personal", "team", "groupchat"],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "composeExtensions": [],
  "configurableTabs": [],
  "staticTabs": [],
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": ["${{BOT_DOMAIN}}"]
}
